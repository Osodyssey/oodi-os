{
  "name": "oodi-starter-secure-v2",
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "build": "node src/cli.js build src/examples/ai_full.oodi -o out-ai.js",
    "build:all": "node src/cli.js build src/examples/ai_full.oodi -o out-ai.js && node src/cli.js build src/examples/todo.oodi -o out-todo.js",
    "start-server": "node server/index.js",
    "dev-server": "nodemon --watch server --exec node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^9.0.0",
    "pegjs": "^0.10.0",
    "minimist": "^1.2.7",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "esbuild": "^0.19.12"
  }
}
